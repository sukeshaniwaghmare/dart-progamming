void addTwoNumbers(int a, int b) {
  int c = a + b;
  print(c);
}

void substractTwoNumber(int a, int b) {
  int c = a - b;
  print(c);
}

void multiplyTwoNumber(int a, int b) {
  int c = a * b;
  print(c);
}

void divideTwoNumber(int a, int b) {
  int c = a ~/ b;
  print(c);
}

//modulus means remainder
void modulusTwoNumber(int a, int b) {
  int c = a ~/ b;
  print(c);
}
